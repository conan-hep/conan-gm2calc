find_library(GM2Calc_LIBRARY NAMES ${CONAN_LIBS_GM2CALC} PATHS ${CONAN_LIB_DIRS_GM2CALC})
find_path(GM2Calc_INCLUDE_DIRS NAMES gm2calc/gm2_config.h PATHS ${CONAN_INCLUDE_DIRS_GM2CALC})

set(GM2Calc_FOUND TRUE)
set(GM2Calc_INCLUDE_DIRS ${GM2CALC_INCLUDE_DIR})
